# PyMOL visualization script for FN1
# Generated by 19_alphafold_structure_mapping.py

load C:\Users\Gestalt\Desktop\ternary\repos\ternary-vaes\research\bioinformatics\rheumatoid_arthritis\results\structural\19_alphafold_mapping\pdb_files\AF-P02751-F1-model_v6.pdb, FN1
hide all
show cartoon, FN1
color gray80, FN1

# Color arginines by entropy change
# Red = entropy increase (immunodominant)
# Blue = entropy decrease (silent)

set_color entropy_FN1_1, [0.831, 0.831, 1.000]
color entropy_FN1_1, FN1 and resi 1
show sticks, FN1 and resi 1
label FN1 and resi 1 and name CA, "R1*"
set_color entropy_FN1_2, [1.000, 0.637, 0.637]
color entropy_FN1_2, FN1 and resi 2
show sticks, FN1 and resi 2
label FN1 and resi 2 and name CA, "R2*"
set_color entropy_FN1_4, [0.812, 0.812, 1.000]
color entropy_FN1_4, FN1 and resi 4
show sticks, FN1 and resi 4

# Styling
bg_color white
set ray_shadows, 0
set antialias, 2
set cartoon_fancy_helices, 1
zoom FN1

# Save image
ray 1920, 1080
png C:\Users\Gestalt\Desktop\ternary\repos\ternary-vaes\research\bioinformatics\rheumatoid_arthritis\results\structural\19_alphafold_mapping/FN1_entropy_map.png, dpi=300